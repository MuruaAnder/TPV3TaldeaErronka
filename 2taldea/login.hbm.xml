<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="2taldea"
                   namespace="_2taldea">

	<class name="Langilea" table="langilea">
		<id name="Id" column="id" type="int">
			<generator class="identity" />
		</id>
		<!-- Los nombres de la propiedades deben de coincidir con los nombres de tu tabla de la base de datos en la propiedad column -->
		<property name="Izena" column="izena" type="string"/>
		<property name="Pasahitza" column="pasahitza" type="string"/>
		<property name="Postua" column="postua" type="string"/>
	</class>

</hibernate-mapping>