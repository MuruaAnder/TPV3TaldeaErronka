<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="2taldea"
                   namespace="_2taldea">

	<class name="Produktua" table="produktua">
		<id name="Id" column="id" type="int">
			<generator class="identity" />
		</id>
		<property name="Stock" column="stock" type="int" />
		<property name="Max" column="max" type="int" />
		<property name="Min" column="min" type="int" />
		<property name="Prezioa" column="prezioa" type="float" />
		<property name="Izena" column="izena" type="string" />
	</class>

</hibernate-mapping>
